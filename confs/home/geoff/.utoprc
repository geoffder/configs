! UTop syntax highlighting
! Attempted to use monokai hexes, but it seems to be rounding colours
! to the nearest present in lambda-term. Tried to replicate some by
! using the 256 Spectrum colour set (used by oh-my-zsh) as a base.

! See: https://github.com/ocaml-community/utop/blob/master/utoprc-dark

! monokai
! magenta = #f92672
! blue    = #66d9ef
! orange  = #fd971f

! molokai
! grey    = #525254

! spectrum mappings
! 9   -> red     -> #ff0000
! 13  -> fuschia -> #ff00ff
! 200 -> gold    -> #ffd700

profile:                  dark
identifier.foreground:    none
module.foreground:        #66d9ef
comment.foreground:       #525254
doc.foreground:           #e2c770
constant.foreground:      #ff00ff
keyword.foreground:       #f92672
symbol.foreground:        x-palegreen
string.foreground:        #ffd700
char.foreground:          #ffd700
quotation.foreground:     #ff00ff
error.foreground:         #ff0000
directive.foreground:     #ff00ff
parenthesis.background:   #fd971f
